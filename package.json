    "monaco-editor": "^0.8.3",
    "rimraf": "^2.0.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^1.0.2",
    "rollup-watch": "^3.2.2",
    "ts-node": "^1.0.0",
    "tslint": "^4.0.0",
    "typescript": "^2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  }
